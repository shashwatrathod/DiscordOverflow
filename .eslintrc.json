{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "useTabs": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "endOfLine": "auto",
        "printWidth": 100,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 2
      }
    ]
  }
}
